{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport socket from \"../socket\";\nexport default {\n  el: '#v-app',\n  data() {\n    return {\n      title: 'MyChat.Vue component',\n      text: '',\n      messages: ['Some message', 'Another messages'],\n      socket: null\n    };\n  },\n  methods: {\n    receiveMessage(msg) {\n      console.log(`recv: ${msg}`);\n      this.messages.push(msg);\n    },\n    sendMessage() {\n      console.log(`send: ${this.text}`);\n      this.socket.emit('msgToServer', this.text);\n      this.text = \"\";\n    }\n  },\n  created() {\n    this.socket = io('http://localhost:3000');\n    this.socket.on('msgToClient', msg => {\n      console.log(`recv: ${msg}`);\n      this.receiveMessage(msg);\n    });\n  }\n};","map":{"version":3,"mappings":";AAmBI,OAAOA,MAAK,MAAO,WAAW;AAC9B,eAAe;EACXC,EAAE,EAAE,QAAQ;EACZC,IAAI,GAAG;IACH,OAAO;MACKC,KAAK,EAAE,sBAAsB;MAC7BC,IAAI,EAAE,EAAE;MACRC,QAAQ,EAAE,CAAC,cAAc,EAAE,kBAAkB,CAAC;MAC9CL,MAAM,EAAE;IACZ,CAAC;EACb,CAAC;EACDM,OAAO,EAAE;IACLC,cAAc,CAACC,GAAG,EAAE;MAChBC,OAAO,CAACC,GAAG,CAAE,SAAQF,GAAI,EAAC,CAAC;MAC3B,IAAI,CAACH,QAAQ,CAACM,IAAI,CAACH,GAAG,CAAC;IAC3B,CAAC;IACDI,WAAW,GAAG;MACVH,OAAO,CAACC,GAAG,CAAE,SAAQ,IAAI,CAACN,IAAK,EAAC,CAAC;MACjC,IAAI,CAACJ,MAAM,CAACa,IAAI,CAAC,aAAa,EAAE,IAAI,CAACT,IAAI,CAAC;MAC1C,IAAI,CAACA,IAAG,GAAI,EAAE;IAClB;EACJ,CAAC;EACDU,OAAO,GAAG;IACN,IAAI,CAACd,MAAK,GAAIe,EAAE,CAAC,uBAAuB,CAAC;IACzC,IAAI,CAACf,MAAM,CAACgB,EAAE,CAAC,aAAa,EAAGR,GAAG,IAAK;MACnCC,OAAO,CAACC,GAAG,CAAE,SAAQF,GAAI,EAAC,CAAC;MAC3B,IAAI,CAACD,cAAc,CAACC,GAAG,CAAC;IAC5B,CAAC;EACL;AACJ","names":["socket","el","data","title","text","messages","methods","receiveMessage","msg","console","log","push","sendMessage","emit","created","io","on"],"sourceRoot":"","sources":["/Users/rooteleven/Desktop/transcendence/frontend/src/components/MyChat.vue"],"sourcesContent":["<template>\n    <div id=\"v-app\">\n        <h1> {{ title }} </h1>\n\n        <form>\n            <input v-model=\"text\" type=\"text\" />\n            <button type=\"submit\" @click.prevent=\"sendMessage()\">Send it</button>\n        </form>\n\n        <p>\n            <ul>\n                <li v-for=\"msg of messages\"> {{ msg }} </li>\n            </ul>\n        </p>\n\n    </div>\n</template>\n\n<script>\n    import socket from \"../socket\";\n    export default {\n        el: '#v-app',\n        data() {\n            return {\n                        title: 'MyChat.Vue component',\n                        text: '',\n                        messages: ['Some message', 'Another messages'],\n                        socket: null,\n                    };\n        },\n        methods: {\n            receiveMessage(msg) {\n                console.log(`recv: ${msg}`);\n                this.messages.push(msg);\n            },\n            sendMessage() {\n                console.log(`send: ${this.text}`);\n                this.socket.emit('msgToServer', this.text);\n                this.text = \"\";\n            }\n        },\n        created() {\n            this.socket = io('http://localhost:3000');\n            this.socket.on('msgToClient', (msg) => {\n                console.log(`recv: ${msg}`);\n                this.receiveMessage(msg);\n            })\n        },\n    }\n</script>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}